<h1>Listing uni_classes</h1>

<table>
  <tr>
    <th>Code</th>
    <th>Title</th>
    <th>Schedule</th>
    <th>Instructor</th>
    <th>Room</th>
    <th>Subject</th>
    <th></th>
    <th></th>
    
  </tr>

<% @classes.each do |uni_class| %>
  <tr>
    <td><%= uni_class.code %></td>
    <td><%= uni_class.title %></td>
    <td><%= uni_class.schedule %></td>
    <td><%= uni_class.instructor %></td>
    <td><%= uni_class.room %></td>
    <td><%= uni_class.subject %></td>
    <td><%= link_to 'Show', uni_class %></td>
    <td><%= link_to 'Remove Class', :controller => "uni_classes" , :action => "deregister", :id => uni_class.id  %></td>
    
  </tr>
<% end %>
</table>
<br />